{
  "name": "isubo-core",
  "version": "0.0.1-alpha.1",
  "description": "core of isubo",
  "main": "index.js",
  "type": "module",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest",
    "esm-test": "npx jest --clearCache && node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
    "only-test": "./jest.js",
    "build": "node ./scripts/build.js",
    "prepare": "husky install",
    "commitlint": "commitlint --edit",
    "postversion": "git push && git push origin --tags",
    "new-version": "git fetch origin main && git checkout main && git pull origin main && npx isubo-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isubo-org/isubo-core.git"
  },
  "keywords": [
    "isubo",
    "bore"
  ],
  "author": "isaaxite",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/isubo-org/isubo-core/issues"
  },
  "homepage": "https://github.com/isubo-org/isubo-core#readme",
  "dependencies": {
    "clipboardy": "^3.0.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "js-yaml": "^4.1.0",
    "markdown-toc": "^1.2.0",
    "octokit": "^2.0.19",
    "remark-frontmatter": "^4.0.1",
    "remark-parse": "^10.0.2",
    "remark-stringify": "^10.0.3",
    "simple-git": "^3.19.0",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.11",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/plugin-transform-optional-chaining": "^7.22.12",
    "@babel/plugin-transform-private-methods": "^7.22.5",
    "@babel/plugin-transform-private-property-in-object": "^7.22.11",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@isubo-org/publish": "0.0.2",
    "@isubo-org/version": "0.0.2-alpha.6",
    "@jest/globals": "^29.5.0",
    "@octokit/request": "^8.1.1",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-stats": "^1.0.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-html-reporter": "^3.10.0",
    "msw": "^1.2.3",
    "plato": "^1.7.0",
    "rollup": "^3.26.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
